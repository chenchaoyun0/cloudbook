    <select
        id="selectByPrimaryKey"
        resultMap="BaseResultMap"
        parameterType="com.cyc.common.po.TLogKey">
        select
        <include refid="Base_Column_List" />
        from t_log
        where log_Id = #{logId,jdbcType=VARCHAR}
        and user_Ip = #{userIp,jdbcType=VARCHAR}
    </select>
