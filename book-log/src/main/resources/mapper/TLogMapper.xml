    <!-- 查询日志分页 -->
    <select
        id="selectLogPagesForIp"
        resultMap="BaseResultMap"
        parameterType="string"
        useCache="false">
        select
        <include refid="Base_Column_List" />
        from t_log where user_Ip=#{userIp,jdbcType=VARCHAR}
        order by oper_Time DESC
    </select>
    <select
        id="selectLogSumCount"
        resultType="java.lang.Long"
        useCache="false">
        select SUM(count) from t_log
    </select>
    <!-- todayCount -->
    <select id="todayCount" resultType="com.cyc.common.vo.TodayCountVo">
        SELECT
          count(*) as todayVisitorCount,
          SUM(sc) as todayCount
        FROM
          (
            SELECT
              *, SUM(count) sc
            FROM
              t_log t
            WHERE
              t.oper_Time BETWEEN #{todayBegin}
            AND #{todayEnd}
            GROUP BY
              t.user_Ip
          ) b
    </select>
</mapper>