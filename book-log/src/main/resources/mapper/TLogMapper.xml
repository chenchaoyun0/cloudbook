        module, action, action_Time,
        oper_Time, count)
        values (#{logId,jdbcType=VARCHAR}, #{userIp,jdbcType=VARCHAR},
        #{userName,jdbcType=VARCHAR},
        #{userNickName,jdbcType=VARCHAR}, #{userAddress,jdbcType=VARCHAR}, #{userJwd,jdbcType=VARCHAR},
        #{module,jdbcType=VARCHAR}, #{action,jdbcType=VARCHAR},
        #{actionTime,jdbcType=BIGINT},
        #{operTime,jdbcType=VARCHAR}, #{count,jdbcType=BIGINT})
    </insert>
