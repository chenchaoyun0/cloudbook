    <update
        id="updateByPrimaryKeySelective"
        parameterType="com.cyc.common.po.TLog">
        update t_log
        <set>
            <if test="userName != null">
                user_Name = #{userName,jdbcType=VARCHAR},
            </if>
            <if test="userNickName != null">
                user_Nick_Name = #{userNickName,jdbcType=VARCHAR},
            </if>
            <if test="userAddress != null">
                user_Address = #{userAddress,jdbcType=VARCHAR},
            </if>
            <if test="userJwd != null">
                user_JWD = #{userJwd,jdbcType=VARCHAR},
            </if>
            <if test="module != null">
                module = #{module,jdbcType=VARCHAR},
            </if>
            <if test="action != null">
                action = #{action,jdbcType=VARCHAR},
            </if>
            <if test="actionTime != null">
                action_Time = #{actionTime,jdbcType=BIGINT},
            </if>
            <if test="operTime != null">
                oper_Time = #{operTime,jdbcType=VARCHAR},
            </if>
            <if test="count != null">
                count = #{count,jdbcType=BIGINT},
            </if>
        </set>
        where log_Id = #{logId,jdbcType=VARCHAR}
        and user_Ip = #{userIp,jdbcType=VARCHAR}
    </update>
    <update
        id="updateByPrimaryKey"
        parameterType="com.cyc.common.po.TLog">
        update t_log
        set user_Name = #{userName,jdbcType=VARCHAR},
        user_Nick_Name = #{userNickName,jdbcType=VARCHAR},
        user_Address = #{userAddress,jdbcType=VARCHAR},
        user_JWD = #{userJwd,jdbcType=VARCHAR},
        module = #{module,jdbcType=VARCHAR},
        action = #{action,jdbcType=VARCHAR},
        action_Time = #{actionTime,jdbcType=BIGINT},
        oper_Time = #{operTime,jdbcType=VARCHAR},
        count = #{count,jdbcType=BIGINT}
        where log_Id = #{logId,jdbcType=VARCHAR}
        and user_Ip = #{userIp,jdbcType=VARCHAR}
    </update>
    <!-- 通过ip查询 -->
    <select
        id="selectByUserIp"
        resultMap="BaseResultMap"
        parameterType="String"
        useCache="false">
        select
        <include refid="Base_Column_List" />
        from t_log
        where user_Ip = #{userIp,jdbcType=VARCHAR} limit 1
    </select>
    <!-- 查询日志分页 -->
    <select
        id="selectLogPages"
        resultMap="BaseResultMap"
        parameterType="com.cyc.common.po.TLog"
        useCache="false">
        SELECT
            log_Id,
            user_Name,
            user_Nick_Name,
            user_Address,
            user_JWD,
            module,
            action,
            action_Time,
            user_Ip,
            MAX(oper_Time) oper_Time,
            SUM(count) as count
        FROM
            t_log 
        GROUP BY user_Ip
        ORDER BY oper_Time DESC
    </select>
